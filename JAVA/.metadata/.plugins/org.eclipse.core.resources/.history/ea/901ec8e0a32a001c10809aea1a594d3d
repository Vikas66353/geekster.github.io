package assignment;

public class greatestNUMBER {
	public static void main(String args[])
	{
		int a = 20;
		int b = 30;
		int c = 50;
		if(a>b && a>c)
		{
			System.out.print("a is greater than b and c");
		}
		else if (b>a && b>c)
		{
			System.out.print("b is greater than a and c");
		}
		else if (c>a && c>b)
		{
			System.out.print("c is greater than b and a");
		}
	}

}
